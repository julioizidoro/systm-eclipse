	<queryString>
		<![CDATA[Select distinct
    vendas.dataVenda,vendas.idvendas,
    unidadeNegocio.razaoSocial, unidadeNegocio.nomeFantasia,usuario.nome as nomeUsuario,
    cliente.nome as nomecliente, cliente.cpf
from
     vendas join trainee on vendas.idvendas = trainee.vendas_idvendas
     join cliente on vendas.cliente_idcliente = cliente.idcliente
     join unidadeNegocio on vendas.unidadeNegocio_idunidadeNegocio = unidadeNegocio.idunidadeNegocio
     join usuario on vendas.usuario_idusuario = usuario.idusuario

where
    vendas.idvendas = $P{idvendas}]]>
	</queryString>
