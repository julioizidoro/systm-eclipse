	<queryString>
		<![CDATA[Select distinct
   vendas.dataVenda, vendas.valor as valorVenda,
   aupair.idaupair,
aupair.dataInscricao,
aupair.possuiAmigosPais,
aupair.nomeAmigo,
aupair.foneAmigo,
aupair.endercoAmigo,
aupair.relacaoAmigo,
aupair.nivelEstudo,
aupair.ocupacao,
aupair.inituicaoEstuda,
